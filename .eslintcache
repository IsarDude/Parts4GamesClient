 useState();\n    const[selectedRam, setSelectedRam]= useState({});\n    const[ramBudget, setRamBudget] = useState();\n\n\n    function selectRam(e) {\n        setSelectedRam(ramList[e.target.dataset.key]);\n    }\n\n    \n    function getRamList(){\n        var url = 'http://localhost:8080/Parts4Games/ramList'; \n        axios.get(url, {params: {ramCapacity: keyword, budget: ramBudget }})\n            .then(res => {\n                setRamList(res.data);\n            }\n            );\n        }\n\n    return(\n        <div style={{width:\"45%\"}}>\n            <h3 className=\"mt-2 mb-2\">Search for a RAM</h3>\n            <InputGroup className=\"mb-3\">\n                <FormControl\n                type = \"number\"\n                placeholder=\"Type in RAM-Capacity\"\n                aria-label=\"Type in RAM-Capacity\"\n                aria-describedby=\"basic-addon2\"\n                onChange={function func(e){\n                    setKeyword(e.target.value);\n                }}\n                value={keyword}\n                />\n                <InputGroup.Append>\n                <Button variant=\"outline-secondary\" onClick={getRamList}>Search</Button>\n                </InputGroup.Append>\n            </InputGroup>\n            <div className=\"d-flex\">\n            <ListGroup style={{width: \"40%\", minHeight:\"15rem\"}}>\n                <ListGroup.Item>\n                    <Row>\n                        <Col><h6>Brand</h6></Col>\n                        <Col><h6>Model</h6></Col>\n                        <Col><h6>Capacity</h6></Col>\n                    </Row>\n                </ListGroup.Item>\n                {ramList.map((ram,j) => {\n                    return(\n                        <ListGroup.Item action data-key={j} onClick={selectRam}>\n                        <Row data-key={j}>\n                            <Col data-key={j}>{ram.brand}</Col>\n                            <Col data-key={j}>{ram.model}</Col>\n                            <Col data-key={j}>{ram.totalcapacity}</Col>\n                        </Row>\n                               \n                        </ListGroup.Item>\n                    )\n                })}\n            </ListGroup>\n            <div className =\"ml-5\">\n            <Form.Label>Budget:</Form.Label>\n            <Form.Control\n                type= \"number\"\n                placeholder=\"Budget\"\n                value = {ramBudget}\n                onChange = {(e) => setRamBudget(e.target.value)}\n                className=\"mb-2\"\n            ></Form.Control>\n            <Card style={{width:'100%', minHeight:\"15rem\"}}>\n                <Card.Body >\n                    <Card.Header>Details</Card.Header>\n                    <ListGroup variant=\"flush\">\n                       { Object.keys(selectedRam).map(key => {\n                           return(\n                           <ListGroup.Item><h5>{key}:</h5> {selectedRam[key]} </ListGroup.Item>\n                           )\n                       })    \n                       }\n    \n                    </ListGroup>\n                    \n                </Card.Body>\n            </Card>\n            <Button onClick={() => props.addRam(selectedRam)}>Add to Config</Button>\n            </div>\n        </div>\n        </div>\n    )\n}\n\nexport default RamList;","/home/student/Client/Parts4GamesClient/src/components/ConfigList.js",["60","61"],"/home/student/Client/Parts4GamesClient/src/components/GameList.jsx",["62","63","64"],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":4,"column":10,"nodeType":"71","messageId":"72","endLine":4,"endColumn":14},{"ruleId":"69","severity":1,"message":"73","line":5,"column":8,"nodeType":"71","messageId":"72","endLine":5,"endColumn":13},{"ruleId":"69","severity":1,"message":"74","line":7,"column":9,"nodeType":"71","messageId":"72","endLine":7,"endColumn":15},{"ruleId":"69","severity":1,"message":"75","